cc_library_static {
    name: "libstagefright_timedtext",

    srcs: ["TextDescriptions.cpp"],

    cflags: [
        "-Wno-multichar",
        "-Werror",
        "-Wall",
        "-DLOG_NDEBUG=0",
    ],

    sanitize: {
        misc_undefined: [
            "signed-integer-overflow",
        ],
        cfi: true,
        diag: {
            cfi: true,
        },
    },

    include_dirs: [
        "frameworks/av/media/libstagefright",
    ],

    shared_libs: ["libmedia"],
}
